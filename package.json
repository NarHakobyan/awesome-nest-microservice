{
    "name": "awesome-nest-microservice",
    "version": "7.0.1",
    "description": "Awesome NestJS Boilerplate, Typescript, Postgres, TypeORM",
    "author": "Narek Hakobyan <narek.hakobyan.07@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build:prod": "tsc -p tsconfig.build.json",
        "postbuild:prod": "copyfiles --up 1 src/**/*.json dist",
        "start:hmr": "node dist/main.hmr.ts",
        "start:dev": "ts-node src/main.ts",
        "start:prod": "node dist/main.js",
        "schema:drop": "yarn run typeorm schema:drop",
        "watch:dev": "nodemon --config nodemon.json",
        "debug:dev": "nodemon --config nodemon-debug.json",
        "webpack": "webpack --config webpack.config.js --progress",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "docs:dev": "vuepress dev -p 7070",
        "docs:build": "DEPLOY_ENV=gh-pages vuepress build",
        "docs:deploy": "yarn docs:build && gh-pages -d .vuepress/dist"
    },
    "dependencies": {
        "@elastic/elasticsearch": "^7.12.0",
        "@nestjs/common": "^7.6.15",
        "@nestjs/config": "^0.6.3",
        "@nestjs/core": "^7.6.15",
        "@nestjs/elasticsearch": "^7.1.0",
        "@nestjs/microservices": "^7.6.15",
        "@nestjs/terminus": "^7.2.0",
        "@nestjs/typeorm": "^7.1.5",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "lodash": "^4.17.21",
        "pg": "^8.6.0",
        "reflect-metadata": "^0.1.13",
        "request-context": "^2.0.0",
        "rxjs": "^6.6.7",
        "source-map-support": "^0.5.19",
        "typeorm": "^0.2.32",
        "typeorm-transactional-cls-hooked": "^0.1.20",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/testing": "^7.6.15",
        "@types/jest": "^26.0.22",
        "@types/lodash": "^4.14.168",
        "@types/node": "^14.14.41",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/eslint-plugin-tslint": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "clean-webpack-plugin": "^3.0.0",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-import-helpers": "^1.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-unicorn": "^32.0.1",
        "gh-pages": "^3.1.0",
        "husky": "^3.0.9",
        "jest": "^26.6.3",
        "lint-staged": "~9.4.2",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.5",
        "ts-loader": "^9.1.1",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-plugin-prettier": "^2.3.0",
        "typescript": "^4.2.4",
        "vuepress": "^1.8.2",
        "webpack": "^5.35.1",
        "webpack-cli": "^4.6.0",
        "webpack-node-externals": "^3.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "git add"
        ]
    }
}
